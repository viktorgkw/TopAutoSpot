@page
@model TopAutoSpot.Views.AdministratorViews.Utilities.ApproveVehicleModel
@{
    ViewData["Title"] = "Vehicle Approved";
}

<h1 class="website-text-darker-nohover fw-bold">Vehicle Approved!</h1>
<hr />

<p class="fs-3 website-text-nohover">Vehicle with Id: @Model.VehicleId was <span class="fw-bold text-success">Approved succesfully</span>!</p>

<a class="btn website-background-outline-main fw-bold custom-rounded" asp-page="/AdministratorViews/Approval">Go Back</a>